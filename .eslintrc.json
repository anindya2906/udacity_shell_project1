{
    "env": {
        "es2021": true,
        "node": true,
        "jasmine": true
    },
    "extends": [
        "standard",
        "plugin:jasmine/recommended",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier",
        "jasmine"
    ],
    "rules": {
        "prettier/prettier": 2
    }
}
